# Docker Environment Variables for EventCarbon
# Copy this file to .env and customize for your environment

# JWT Secrets (CHANGE THESE IN PRODUCTION!)
JWT_ACCESS_SECRET=your-super-secure-access-secret-key-minimum-32-chars
JWT_REFRESH_SECRET=your-super-secure-refresh-secret-key-minimum-32-chars

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:8065

# API URL (used by frontend)
VITE_API_URL=http://localhost:8035/api

# OAuth - Google (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# OAuth - GitHub (optional)
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

